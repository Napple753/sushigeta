        "@vue/tsconfig": "^0.5.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.0",
        "eslint-plugin-vue": "^9.20.0",
        "husky": "^9.0.0",
        "jsdom": "^24.0.0",
        "lint-staged": "^15.2.0",
        "prettier": "^3.2.0",
        "sass": "^1.93.2",
        "storybook": "^7.6.0",
        "typescript": "^5.3.0",
